{
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": ["react-hot-loader/babel", ["import", {
            "libraryName": "antd",
            "style": true
        }], "@babel/proposal-function-bind", ["@babel/proposal-decorators", {
            "legacy": true,
        }],
        ["@babel/proposal-class-properties", {
            "loose": true
        }], "@babel/syntax-dynamic-import","@babel/transform-runtime"
    ],
    "env":{
        "test":{
            "plugins":[
                "istanbul"
            ]
        }
    }
}